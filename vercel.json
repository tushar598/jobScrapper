{
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/api(.*)",
      "dest": "/server/src/app.js"
    },
    {
      "src": "^/(?!api)(.*)",
      "dest": "/client/dist/$1"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "GOOGLE_API_KEY": "@GOOGLE_API_KEY",
    "JWT_SECRET": "@JWT_SECRET"
  }
}
